  <script>
    function changeQuantity(cartid, proId, quantity, count) {
      $.ajax({
        url: '/change-product-quantity',
        data: {
          cartid: cartid,
          product: proId,
          quantity: quantity,
          count: count
        }, method: 'post',
        success: (response) => {
          if (response.removeProduct) {           
            location.reload();
          } else {
            location.reload();
          }

        }
      })
    } 
  </script>
 
