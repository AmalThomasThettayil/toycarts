<body>
  

  <!--================Home Banner Area =================-->
  <!-- breadcrumb start-->
  <section class="breadcrumb breadcrumb_bg">
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-lg-8">
          <div class="breadcrumb_iner">
            <div class="breadcrumb_iner_item" style="position: relative">
              <h2>Cart Products</h2>
              <p>Home <span>-</span>Cart Products</p>
              <img src="images/cat.png" alt="" style="position: absolute" height="350px">
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- breadcrumb start-->

  <!--================Cart Area =================-->
  {{#if cartCount}}
  <section class="cart_area padding_top">
    <div class="container">
      <div class="cart_inner">
        <div class="table-responsive">
          <table class="table">
            <thead>
              <tr>
                <th scope="col">Product</th>
                <th scope="col">Price</th>
                <th scope="col">Quantity</th>
                <th scope="col">Total</th>
              </tr>
            </thead>

            <tbody>
              {{#each cartItems.products}}
              <tr>

                <td>

                  <div class="media">
                    <div class="d-flex">
                      <img src="/imgUploads/{{pro_Id.image.[0].image1}}" alt="" width="200" height="200" />
                    </div>
                    <div class="media-body">
                      <p>{{pro_Id.productName}}</p>
                    </div>
                  </div>
                </td>
                <td>
                  <h5>Rs.{{pro_Id.price}}</h5>
                </td>
                <td style="width:150px">
                  <button class="btn btn-outline-secondary btn-outline-danger" type="button"
                    onclick="changeQuantity('{{this._id}}','{{pro_Id._id}}','{{this.quantity}}',-1)">
                    <i class="fs-12 fa fa-minus" aria-hidden="true"></i></button>
                  <span>{{this.quantity}}</span>
                  <button class="btn btn-outline-success btn-outline-secondary"
                    onclick="changeQuantity('{{this._id}}','{{pro_Id._id}}','{{this.quantity}}',1)" type="button">
                    <i class="fs-12 fa fa-plus" aria-hidden="true"></i></button>
                </td>
                <td>
                  <h5>{{subTotal}}</h5>
                </td>
                <td>
                  <button class="btn btn-outline-secondary btn-outline-danger" type="button"
                    onclick="removeProductFromCart('{{this._id}}')">
                    <i class="fa-solid fa-xmark"></i></button>
                </td>
              </tr>
              {{/each}}
              <tr>
                <td></td>
                <td></td>
                <td>
                  <h5>Net Total</h5>
                </td>
                <td>
                  <h5>Rs.{{netTotal}}/-</h5>
                </td>
              </tr>
              <tr class="shipping_area">
                <td></td>
                <td></td>
                <td>Shipping charge</td>
                <td>
                  <h5>&nbsp;Rs.{{deliveryCharge}}/-</h5>
                </td>
              </tr>
              <tr>
                <td></td>
                <td></td>
                <td>Order total</td>
                <td>
                  <div class="shipping_box">
                    <ul class="list">
                      <li>
                        <h4>&nbsp;Rs.{{grandTotal}}/-</h4>
                      </li>
                    </ul>
                  </div>
                </td>
              </tr>
            </tbody>
          </table>
          <div class="checkout_btn_inner float-right">
            <a class="btn_1" href="/">Continue Shopping</a>
            <a class="btn_1 checkout_btn_1" href="/checkout">Proceed to checkout</a>
          </div>
        </div>
      </div>
  </section>
  {{else}}
  <section class="cart bgwhite pb-100">
    <div class="container">
      <div class="card m-auto text-center" style="width: 20rem; border:0px">
        {{!-- <img src="/https://www.pinterest.com/pin/424253227381104763/" class="card-img-top  m-auto"
          style="width: 6rem;" alt="..."> --}}
        <div class="card-body">
          <h4 class="card-title font-weight-bold">Your Cart is Empty</h4>
          <p class="card-title font-weight-bold">Add something to make me happy!</p>
          <a href="/" class="btn btn-primary">Continue shopping</a>
        </div>
      </div>
    </div>
  </section>

  {{/if}}
  <!--================End Cart Area =================-->

