<!DOCTYPE html>
<html>

<head>
    <title>{{title}}</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <!-- Font Awesome -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" rel="stylesheet" />
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" rel="stylesheet" />
    <!-- MDB -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/4.0.0/mdb.min.css" rel="stylesheet" />

    {{!-- validation --}}
    <script src="https://code.jquery.com/jquery-3.2.1.js"
        integrity="sha256-DZAnKJ/6XZ9si04Hgrsxu/8s717jcIzLy3oi35EouyE=" crossorigin="anonymous"></script>
    {{!--
    <link rel='stylesheet' href='/stylesheets/style.css' /> --}}
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.0/jquery.validate.min.js"></script>
    {{!-- validation --}}
</head>


<body>
    <section class="h-100" style="background-color: #fbff0079;">
        <div class="container py-5 h-100">
            <div class="row d-flex justify-content-center align-items-center h-100">
                <div class="col col-xl-10">
                    <div class="card" style="border-radius: 1rem;">
                        <div class="row g-0">
                            <div class="col-md-6 col-lg-5 d-none d-md-block">
                                <img src="https://m.media-amazon.com/images/I/71xXBXUCQ5L._SX569_.jpg" alt="login form"
                                    class="img-fluid pt-5 mt-5" style="border-radius: 1rem 0 0 1rem;" />
                            </div>
                            <div class="col-md-6 col-lg-7 d-flex align-items-center">
                                <div class="card-body p-lg-5 text-black">
                                    <form action="/signUp" method="post" id="forms">
                                        <div class="d-flex justify-content-center mb-3 pb-1">
                                            <a href="/">
                                                <img class=""
                                                    src="data:image/jpeg;base64,"
                                                    alt="">
                                            </a>
                                        </div>
                                        <!-- Name input -->
                                        <div class="col-md-12 form-group p_star pb-3">
                                            <input type="text" class="form-control" id="name" name="name" value=""
                                                placeholder="Username">
                                        </div>
                                        <div class="col-md-12 form-group p_star">
                                            <input type="email" class="form-control" id="loginName" name="email"
                                                value="" placeholder="Email">
                                        </div>
                                        <p style="color: red;">{{signuperror}}</p>
                                        <div class="col-md-12 form-group p_star pb-3">
                                            <input type="number" class="form-control " id="phone" name="phone" value=""
                                                placeholder="Phone Number">
                                        </div>
                                        <div class="col-md-12 form-group p_star pb-3">
                                            <input type="password" class="form-control" id="password1" name="password"
                                                value="" placeholder="Password">
                                        </div>
                                        <p style="color: red;">{{passErr}}</p>
                                        <div class="col-md-12 form-group p_star pb-3">
                                            <input type="password" class="form-control" id="password2"
                                                name="newpassword" value="" placeholder="Confirm Password">
                                        </div>
                                        <div style="margin-top: 7px;" id="CheckPasswordMatch"></div>
                                        <div class="pt-1 mb-3">
                                            <button class="btn btn-warning btn-lg btn-block"
                                                type="submit">SignUp</button>
                                            <p class="mt-3 pb-lg-2" style="color:#464545;">Already have an account? <a
                                                    href="/uLogin" style="color: #ff0000;">Login here!</a></p>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js"
        integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js"
        integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF"
        crossorigin="anonymous"></script>

    {{!-- validation --}}
    <script>

        $(function () {

            // First: Add with default messages
            //$("#frm-add-data").validate();				

            // Second: Change default messages
            $("#forms").validate({
                errorClass: 'Errors',
                rules: { // to define rules
                    name: {
                        required: true,
							{{!-- lettersonly: true --}}
						},

            password: {
            required: true,
            minlength: 4
        },
            newpassword: {
            required: true,
            minlength: 4
        },
            email: {
            required: true,
            email: true
        },

            phone: {
            required: true,
            maxlength: 10,
            minlength: 10

        }
        },
            messages: { // to define custom messages
            name: "Enter your name",
			{{!-- lettersonly: 'please Enter letters only' --}}

            password: {
            required: "Enter password",
            minlength: "Enter minimum 4 digits"
        },
            newpassword: {
            required: "Confirm password",
            minlength: "Enter minimum 4 digits"
        },
            email: {
            required: 'Enter Email',
            email: "The email should be in the format: abc@domain.tld"
        },
            phone: {
            required: "Enter mobile number",
            maxlength: "Enter 10 digits",
            minlength: "Enter only 10 digits"

        }
                    },
            submitHandler: function (form) {
                if ($(form).valid())
                    form.submit();
                return false; // prevent normal form posting
            }
                });
			});
    </script>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.2/jquery.validate.min.js"></script>
    {{!-- validation --}}
    <script>
        $(document).ready(function () {
            $("#password2").on('keyup', function () {
                var password = $("#password1").val();
                var confirmPassword = $("#password2").val();
                if (password != confirmPassword)
                    $("#CheckPasswordMatch").html("Password does not match !").css("color", "red");
                else
                    $("#CheckPasswordMatch").html("Password match !").css("color", "green");
            });
        });
    </script>



</body>